<p style="color: green"><%= notice %></p>

<% content_for :title, "GameSessions" %>

<h1>Game Sessions</h1>

<div id="game_sessions">
  <% @game_sessions.each do |game_session| %>
    <%= render game_session %>
    <p>
      <%= link_to "Show this game session", game_session %>
    </p>
  <% end %>
</div>

<%= link_to "New game session", new_game_session_path %>
<%= link_to "Back to players", players_path %>
