  <p class="notice"><%= notice %></p>

  <% content_for :title, "Players" %>


  <h1 class='headline'>Players</h1>

    <div id="players">
      <% @players.each do |player| %>
        <%= render player %>
      <% end %>
    </div>

  <%= link_to "New player", new_player_path, class: 'button-new' %>
  <%= link_to "Go to game sessions", game_sessions_path if cookies[Constants::AUTH_COOKIE]%>
  <%= button_to "Logout", logout_player_path, method: :delete if cookies[Constants::AUTH_COOKIE]%>
